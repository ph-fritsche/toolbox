{
    "include": [
        "src",
        "util",
    ],
    "exclude": [
    ],
    "compilerOptions": {
        "noEmit": true,
        "outDir": "dist",
        "declaration": true,
        "target": "ES2022",
        "moduleResolution": "Node",
        "module": "ESNext",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "esModuleInterop": true,
        "allowJs": true,
        "lib": [
            "ESNext",
            "DOM",
        ],
        "types": [
            "node",
        ],
    },
}
